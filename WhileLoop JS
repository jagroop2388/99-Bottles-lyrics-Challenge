
    var Wine = 99;
    while (Wine >= 0) {
        var WineWord = "Bottles";
        if (Wine === 1) {
            WineWord = "Bottles";
        }
       
            console.log(Wine + " "+ WineWord +" of Wine on the wall");
            console.log ("Take one down, and pass it arround");
            Wine--;
        console.log (Wine + " "+ WineWord + " "+ "of Wine on the wall");
        }
        
